{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.3",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "30px",
                    "items": [
                        {
                            "type": "Image",
                            "url": "data:image/png;base64,",
                            "width": "30px",
                            "size": "Small"
                        }
                    ]
                },
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "TextBlock",
                            "text": "Top Tips",
                            "wrap": true,
                            "size": "Large",
                            "horizontalAlignment": "Left",
                            "weight": "Bolder"
                        }
                    ]
                }
            ]
        },
        {
            "type": "TextBlock",
            "text": "View our top tips tips below. From Productivity, Health and Wellbeing and more, we have plenty of tips to help you be more successful. ",
            "wrap": true,
            "spacing": "Medium"
        },
        {
            "type": "Container"
        },
        {
            "type": "ColumnSet",
            "$data": "${$root.tips}",
            "columns": [
                {
                    "type": "Column",
                    "width": "60px",
                    "items": [
                        {
                            "type": "Image",
                            "url": "${imageUrl}",
                            "width": "60px",
                            "height": "60px"
                        }
                    ]
                },
                {
                    "type": "Column",
                    "width": 75,
                    "items": [
                        {
                            "type": "TextBlock",
                            "text": "${title}",
                            "wrap": true,
                            "weight": "Bolder",
                            "size": "Medium"
                        },
                        {
                            "type": "TextBlock",
                            "text": "${tipType}",
                            "wrap": true,
                            "size": "Default",
                            "weight": "Bolder"
                        },
                        {
                            "type": "TextBlock",
                            "text": "${description}",
                            "wrap": true
                        },
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "stretch"
                                }
                            ],
                            "spacing": "Small"
                        }
                    ],
                    "verticalContentAlignment": "Center"
                }
            ],
            "spacing": "Medium",
            "separator": true,
            "style": "emphasis"
        },
        {
            "type": "ActionSet",
            "actions": [
                {
                    "type": "Action.OpenUrl",
                    "title": "View all tips",
                    "url": "${allTipsURL}"
                }
            ]
        }
    ]
}